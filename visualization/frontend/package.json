{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "4.10.2",
    "axios": "0.19.2",
    "chart.js": "2.9.3",
    "http-proxy-middleware": "1.0.4",
    "prop-types": "^15.7.2",
    "react": "16.13.1",
    "react-chartjs-2": "2.9.0",
    "react-csv-reader": "^3.1.1",
    "react-dom": "16.13.1",
    "react-grid-layout": "0.18.3",
    "react-resizable": "1.10.1",
    "react-scripts": "3.4.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --watchAll=false --coverage --env=jest-environment-jsdom-fifteen",
    "eject": "react-scripts eject",
    "lint": "eslint src",
    "format": "prettier src",
    "fix": "eslint src --fix && prettier src --write "
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "lint-staged": {
    "*.{js,jsx,css,html,md,json,test.jsx,test.js}": [
      "prettier --write",
      "git add"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn test --no-cache"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/constants/*.js",
      "!src/setupProxy.js",
      "!src/setupTests.js",
      "!src/index.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "coverageReporters": [
      "text",
      "json",
      "lcov"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/dom": "7.20.0",
    "@testing-library/jest-dom": "5.11.0",
    "@testing-library/react": "10.4.4",
    "@testing-library/react-hooks": "3.3.0",
    "@testing-library/user-event": "12.0.11",
    "@typescript-eslint/eslint-plugin": "2.x",
    "@typescript-eslint/parser": "2.x",
    "babel-eslint": "10.x",
    "canvas": "2.6.1",
    "eslint": "6.x",
    "eslint-config-airbnb": "18.2.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-config-react-app": "5.2.1",
    "eslint-plugin-flowtype": "4.x",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-react": "7.x",
    "eslint-plugin-react-hooks": "2.x",
    "husky": "4.2.5",
    "jest-canvas-mock": "2.2.0",
    "jest-environment-jsdom-fifteen": "^1.0.2",
    "lint-staged": "10.2.11",
    "prettier": "2.0.5",
    "pretty-quick": "2.0.1",
    "react-test-renderer": "16.9.0"
  }
}
